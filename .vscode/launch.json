{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "stopOnEntry": false,
      "args": ["${file}"],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src"
      },
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "protocol": "inspector",
      "console": "internalConsole",
      "outFiles": []
    }
  ]
}
